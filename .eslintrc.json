{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parser":"babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes": ["error", "double"],
        "indent": ["warn", 4],
        "no-empty-function": "off",
        "no-empty": "off",
        "no-var": "off",
        "no-unused-vars": "off",
        "react/prop-types": "off",
        "no-import-assign": "off"
    }
}
